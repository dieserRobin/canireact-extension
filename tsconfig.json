{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
      "module": "esnext",
      "moduleResolution": "node",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
      "noImplicitAny": false,
      "noImplicitThis": false,
      "strictNullChecks": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  